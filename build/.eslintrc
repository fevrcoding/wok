{
    "extends": "../.eslintrc",

    "env": {
        "node": true
    },

    "rules": {
        "one-var": 0,
        "no-new": 0,
        "func-names": 0,
        "prefer-template": 0,
        "global-require": 0,
        "object-shorthand": 0,
        "arrow-body-style": 0,
        "import/no-extraneous-dependencies": [2, {"devDependencies": true, "optionalDependencies": true, "peerDependencies": true}]

    }
}